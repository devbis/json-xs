	if (!(
           SvROK ($arg)
           && SvOBJECT (SvRV ($arg))
           && (SvSTASH (SvRV ($arg)) == JSON_STASH || sv_derived_from ($arg, \"JSON::XL\"))
        ))
          croak (\"object is not of type JSON::XL\");
